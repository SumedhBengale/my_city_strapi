{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "singularName": "review",
    "pluralName": "reviews",
    "displayName": "Review"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "profile_photo": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "name": {
      "type": "string"
    },
    "review": {
      "type": "text"
    },
    "date": {
      "type": "date"
    },
    "rating": {
      "type": "integer",
      "max": 5,
      "min": 1,
      "default": 3
    }
  }
}
